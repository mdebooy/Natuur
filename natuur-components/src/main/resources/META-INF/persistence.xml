<persistence version="2.0"
             xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
                                 http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="natuur" transaction-type="JTA">
    <jta-data-source>jdbc/Natuur</jta-data-source>

    <class>eu.debooy.natuur.domain.DetailDto</class>
    <class>eu.debooy.natuur.domain.FotoDto</class>
    <class>eu.debooy.natuur.domain.GebiedDto</class>
    <class>eu.debooy.natuur.domain.RangDto</class>
    <class>eu.debooy.natuur.domain.TaxonDto</class>
    <class>eu.debooy.natuur.domain.TaxonnaamDto</class>
    <class>eu.debooy.natuur.domain.WaarnemingDto</class>
  </persistence-unit>
</persistence>
