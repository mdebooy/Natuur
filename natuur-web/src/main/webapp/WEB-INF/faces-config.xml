<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd" 
            version="2.0">
  <application>
    <el-resolver>eu.debooy.doosutils.components.EmptyToNullStringELResolver</el-resolver>
  </application>

  <converter>
    <converter-id>GebiedConverter</converter-id>
    <converter-class>eu.debooy.natuur.converter.GebiedConverter</converter-class>
  </converter>
  <converter>
    <converter-id>TaxonConverter</converter-id>
    <converter-class>eu.debooy.natuur.converter.TaxonConverter</converter-class>
  </converter>

  <lifecycle>
    <phase-listener>eu.debooy.doosutils.components.DoosPhaseListener</phase-listener>
  </lifecycle>
</faces-config>
